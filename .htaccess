Options +FollowSymLinks

RewriteEngine On
RewriteBase /

RewriteRule ^api/pokemon/([^/]+)$ pokemonIvanTorres.php?/api/pokemon/id=$1 [L]
RewriteRule ^api/pokemon$ pokemonIvanTorres.php?/api/pokemon [L]
RewriteRule ^api/type$ pokemonIvanTorres.php?/api/type [L]
RewriteRule ^api/type/([^/]+)$ pokemonIvanTorres.php?/api/type/id=$1 [L]
